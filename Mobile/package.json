{
  "name": "WKTaxApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "./script/storybook.sh cleanup && react-native run-ios",
    "test": "yarn run test:lint && yarn test:format && yarn run test:unit",
    "test:lint": "eslint src",
    "test:unit": "jest --coverage",
    "test:format": "yarn prettier --check",
    "clear-node": "rm -rf node_modules",
    "clear-watches": "watchman watch-del-all",
    "clear": "yarn run clear-node && yarn run clear-watches && yarn cache clean",
    "rebuild": "yarn run clear && yarn && react-native link",
    "start:metro": "react-native start",
    "lint": "tsc --noEmit && eslint --ext .ts,.tsx ./",
    "lint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "start": "react-native start",
    "fix:format": "yarn run prettier --write -l",
    "fix:lint": "yarn run test:lint --fix",
    "format": "yarn run fix:lint --fix && yarn run fix:format --fix && prettier --ignore-path '.gitignore' \"./src/**/*.+(js|jsx|ts|tsx)\"",
    "type-check": "tsc",
    "test:report": "jest --ci --reporters=default --reporters=jest-junit --coverage",
    "ci-test": "jest --ci --coverage --reporters=jest-junit",
    "pod-install": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install && cd .."
  },
